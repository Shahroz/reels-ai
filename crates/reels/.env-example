APP_ENV=development
PORT=8080
RUST_BACKTRACE=1
DUMMY=3

ZYTE_API_KEY=XXXX
OPENAI_API_KEY=XXXX
SENTINEL_API_KEY=XXXX
SENTINEL_API_BASE=https://sentinel.bounti.ai
LIGHTHOUSE_URL=https://lighthouse-server-208984921666.us-central1.run.app
LIGHTHOUSE_API_KEY=XXXX
GEMINI_API_KEY=XXXX
STRIPE_SECRET_KEY=XXXX
STRIPE_WEBHOOK_SECRET=
STRIPE_PUBLISHABLE_KEY=XXXX
STRIPE_TRIAL_PERIOD_DAYS=
SERPER_API_KEY=XXXX

# Google OAuth2 Configuration for user authentication
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URL=http://localhost:8081/auth/google/callback

# Frontend URL for OAuth2 redirects and email links
# Can include paths like /real-estate for OAuth, but password reset emails
# will automatically use just the origin (protocol + domain + port)
FRONTEND_URL=http://localhost:5173

# GenNodes workflow execution engine configuration
GENNODES_BASE_URL=http://localhost:3003
GENNODES_USERNAME=gennodes
GENNODES_PASSWORD=gennodes

# Database URL (update based on your Makefile/setup)
DATABASE_URL=postgresql://localuser:localpassword@localhost:5447/localdatabase
APP_ENV=development
JWT_SECRET=XXXX
GCS_BUCKET_MICROSITES=bounti_prod_narrativ_public
GCS_BUCKET=bounti_prod_narrativ_public
GCP_SERVICE_ACCOUNT_PATH=/path/to/your/service_account.json

LAUNCH_DARKLY_ENV_KEY=development
LAUNCH_DARKLY_CLIENT_ID=
LAUNCH_DARKLY_SDK_KEY=
LAUNCH_DARKLY_MOBILE_KEY=

# Stripe metadata product type
STRIPE_METADATA_PRODUCT_TYPE=narrativ

# Dub Attribution Tracking Configuration
DUB_API_KEY=your_dub_api_key_here
DUB_WORKSPACE_ID=your_dub_workspace_id_here
DUB_ENABLED=true
DUB_BASE_URL=https://api.dub.co
DUB_TIMEOUT_SECONDS=30

# Imageboard integration
# Base URL of the Imageboard service (e.g., http://localhost:3000)
IMAGEBOARD_BASE_URL=
# Optional admin API key for creating boards with pre-seeded assets
IMAGEBOARD_API_KEY=

# Email Service Configuration (Postmark)
# Server token for authenticating with Postmark API
POSTMARK_SERVER_TOKEN=your_postmark_server_token_here
# The "from" email address that will appear on sent emails
POSTMARK_FROM_EMAIL=support@yourdomain.com

# Imageboard webhook configuration
IMAGEBOARD_WEBHOOK_TTL_HOURS=24
IMAGEBOARD_WEBHOOK_JWT_SECRET=XXXX
